drones extends layout

block content
  h1= title
  p Welcome to the Drones search results page.

  table
    thead
      tr
        th Model
        th Range (km)
        th Brand
    tbody
      each drone in drones
        tr
          td= drone.model
          td= drone.range_km
          td= drone.brand
<img src="/images/DJI _Phantom _4.png" alt="DJI_Phantom_4" width="300" height="300">
<img src="/images/parrotANAFI.jpg" alt="parrotANAFI" width="300" height="300">
<img src="/images/Autel.png" alt="AUTEL" width="300" height="300">


